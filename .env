VITE_USE_MOCK=false

# Primary API Configuration
# Change this to your actual backend server URL
VITE_API_BASE_URL=http://192.168.1.132:8000/api

# Backend Server URLs
VITE_BACKEND_BASE_URL=http://192.168.1.132:8000
